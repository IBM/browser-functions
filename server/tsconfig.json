{
    "compilerOptions": {
      "target": "es6",
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "../dist",
      "typeRoots": [
        "../node_modules/@types",
        "./@types"
      ]
    },
  }
  